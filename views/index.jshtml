      /* Global Styles
      ----------------------------------------------------------------------------------------------------*/

      html { font-size: 100.01%; }
      body { color: #565656; font:14px/1.5 Arial, Helvetica, sans-serif;min-width:600px; }
      p { line-height: 150%; margin: 0 0 10px; }
      strong { font-weight: bold; }
      em { font-style: italic; }
